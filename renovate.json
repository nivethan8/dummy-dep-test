{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "enabled": false,
      "description": "Disable automatic PRs - only create PRs for explicitly enabled packages"
    },
    {
      "matchPackageNames": [
        "zap"
      ],
      "enabled": true,
      "allowedVersions": ">=latest",
      "labels": [
        "security",
        "devrev:don:core:dvrv-us-1:devo/15U9WWPkGG:issue/6"
      ],
      "prTitle": "{{{depName}}} security update to {{{newVersion}}} [DevRev: don:core:dvrv-us-1:devo/15U9WWPkGG:issue/6]",
      "prBodyNotes": [
        "DevRev Ticket: don:core:dvrv-us-1:devo/15U9WWPkGG:issue/6",
        "Security vulnerability fix"
      ],
      "commitMessageSuffix": "[DevRev: don:core:dvrv-us-1:devo/15U9WWPkGG:issue/6]"
    }
  ]
}